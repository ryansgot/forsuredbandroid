<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Class name="~.*\.R\$.*"/>
    </Match>
    <Match>
        <Class name="~.*\.Manifest\$.*"/>
    </Match>

    <!-- Since FSContentValues is a wrapper to make a ContentValues that implements RecordContainer, this is fine -->
    <Match>
        <Class name="com.fsryan.forsuredb.provider.FSContentValues" />
        <Bug pattern="EQ_CHECK_FOR_OPERAND_NOT_COMPATIBLE_WITH_THIS" />
    </Match>

    <!-- The array is being used to call a ContentProvider method that accepts nulls -->
    <Match>
        <Class name="com.fsryan.forsuredb.ContentProviderQueryable" />
        <Method name="formatProjection" />
        <Bug pattern="PZLA_PREFER_ZERO_LENGTH_ARRAYS" />
    </Match>
</FindBugsFilter>